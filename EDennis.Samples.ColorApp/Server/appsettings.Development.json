{
  "mcp": null,
  "Security": {
    "DefaultPolicies": {
      "ClaimTypes": [ "scope" ]
    },
    "MockClaimsPrincipals": {
      "Maria": {
        "email": [ "maria@a.test" ],
        "role": [ "Admin" ],
        "scope": [ "EDennis.Samples.ColorApi.*" ]
      },
      "Darius": {
        "email": [ "darius@b.test" ],
        "role": [ "Readonly" ],
        "scope": [ "EDennis.Samples.ColorApi.*.Get*" ]
      }
    }
  },
  "CachedTransaction": {
    "EnabledForClaims": {
      "email": [ "maria@a.test" ]
    }
  },
  "DepEndpoints": {
    "https://localhost:44336": [ "RgbController" ]
  },
  "ConnectionStrings": {
    "DomainIdentityDbContext": "Server=(localdb)\\mssqllocaldb;Database=AspNetIdentityServer;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "Serilog": {
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Warning",
          "Microsoft.AspNetCore.Authentication": "Information"
        }
      },
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "File",
          "Args": { "path": "Logs/log.txt" }
        }
      ],
      "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
      "Destructure": [
        {
          "Name": "ToMaximumDepth",
          "Args": { "maximumDestructuringDepth": 4 }
        },
        {
          "Name": "ToMaximumStringLength",
          "Args": { "maximumStringLength": 100 }
        },
        {
          "Name": "ToMaximumCollectionCount",
          "Args": { "maximumCollectionCount": 10 }
        }
      ],
      "Properties": {
        "Application": "EDennis.Samples.ColorApp.Server"
      }
    }
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    },
    "Clients": {
      "EDennis.Samples.ColorApp.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*"
}
